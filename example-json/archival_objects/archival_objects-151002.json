  "notes": [
    {
      "jsonmodel_type": "note_multipart",
      "persistent_id": "7709e062c1b4533ae551aee6df137302",
      "type": "acqinfo",
      "subnotes": [
        {
          "jsonmodel_type": "note_text",
          "content": "The optical discs (DVDs) containing these digital photographs were transferred from Homewood Photography in 2014 and 2017.",
          "publish": false
        }
      ],
      "publish": false
    },
    {
      "jsonmodel_type": "note_multipart",
      "persistent_id": "bf8b464229c4a8639c816867516716c0",
      "type": "arrangement",
      "subnotes": [
        {
          "jsonmodel_type": "note_text",
          "content": "The series is arranged alphabetically by the office or department that requested the shoot. In cases where the client is unknown, the most closely related office or department is used.",
          "publish": false
        }
      ],
      "publish": false
    },
    {
      "jsonmodel_type": "note_multipart",
      "persistent_id": "4b670c7c961ac14343ccecb50c3a6ae6",
      "type": "processinfo",
      "subnotes": [
        {
          "jsonmodel_type": "note_text",
          "content": "This series was processed by Elizabeth England in 2017 and 2018, with the support of a National Digital Stewardship Residency grant, funded by the Institute of Museum and Library Services.",
          "publish": false
        }
      ],
      "publish": false
    },
    {
      "jsonmodel_type": "note_multipart",
      "persistent_id": "3db3270cffa56099b1a3f0b39d360707",
      "type": "appraisal",
      "subnotes": [
        {
          "jsonmodel_type": "note_text",
          "content": "Computer-automated sampling techniques were used in processing this series of digital photographs. Shoots that were in-scope for retention were sampled down to 10% of the original volume, accomplished through systematically retaining every 10th digital image. Athletic events, individual and group portraits were further sampled to retain only representative samples of the contents.",
          "publish": false
        }
      ],
      "publish": false
    },
    {
      "jsonmodel_type": "note_multipart",
      "persistent_id": "9145b929feba05f41c1e1f2426330351",
      "type": "originalsloc",
      "subnotes": [
        {
          "jsonmodel_type": "note_text",
          "content": "The original DVDs exist for all content. In some cases, there is no container profile (box and dvd) associated to the archival object because the database received from Homewood Photo isn't comprehensive. When processing this series, Homewood Photo's database was used to associate the archival objects to the containers as best as possible.",
          "publish": false
        }
      ],
      "publish": false
    },
    {
      "jsonmodel_type": "note_multipart",
      "persistent_id": "bf54b1a7edd2a2444c0db0f5f8dfdb63",
      "type": "scopecontent",
      "subnotes": [
        {
          "jsonmodel_type": "note_text",
          "content": "The Homewood Photography digital photographs series consists entirely of born-digital images ranging in date from 2004-2009. Topics include students and faculty, including portraits of individuals, lectures, awards, and receptions; university events, buildings, commemorations, dedications, and commencements; alumni and alumnae events; and other topics related to Homewood campus life, the student body, faculty, community programs, and campus events. There are approximately 22,000 unique images in the series.",
          "publish": false
        }
      ],
      "publish": false
    },
